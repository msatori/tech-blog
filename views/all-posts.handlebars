<div class="posts-container container-fluid">
  {{#each posts}}
  <div class="card">
    <h3 class="card-title">
      <a href="/post/{{ id }}">{{ title }}</a>
    </h3>
    <div class="card-source">
      Created By {{ User.username }} 
    </div>
    <div class="card-body">
      {{ body }}
    </div>
  </div>
  {{/each}}
</div>
